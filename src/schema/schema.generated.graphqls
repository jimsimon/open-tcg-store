type Card {
  id: String!
  inventory: [Inventory!]!
  name: String!
  thumbnail: String
}

type Inventory {
  condition: String!
  price: String!
  quantity: Int!
}

type Mutation {
  firstTimeSetup(settings: Settings!, userDetails: UserDetails!): String!
}

type Query {
  getSingleCardInventory(searchTerm: String): [Card!]!
  isSetupPending: Boolean!
}

input Settings {
  country: String!
  state: String!
}

input UserDetails {
  email: String!
  firstName: String!
  password: String!
}