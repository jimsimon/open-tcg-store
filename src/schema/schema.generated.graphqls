type Card {
  avatar: String
  id: String!
  inventory: [Inventory]!
  name: String!
}

type Inventory {
  condition: String!
  quantity: Int!
}

type Mutation {
  firstTimeSetup(settings: Settings!, userDetails: UserDetails!): String!
}

type Query {
  getSingleCardInventory: [Card]!
  isSetupPending: Boolean!
}

input Settings {
  country: String!
  state: String!
}

input UserDetails {
  email: String!
  firstName: String!
  password: String!
}